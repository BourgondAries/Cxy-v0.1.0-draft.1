#cxy start

#cxy stop



push a
push b

cp a "start:"
cp b  "
    int main(int argc, char *argv[])
    {
"

rep a b


push x
push y

cp y "3202"
mov x y
cp x "1"

:loop

    inc x
    lt x y
    show lt
    if lt
    begin
        goto out
    end

goto loop

:out

pop x
pop y

cp a "print"
cp b "std::cout <<"
rep a b

cp a "finish"
cp b "}"
rep a b

cp a "The amount of counts: "
show a
cp a "{"
cnt a
show cnt


